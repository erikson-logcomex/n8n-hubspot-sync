{
  "id": 1,
  "uid": "n8n-monitoring-corrected",
  "title": "Monitoramento N8N - Logcomex",
  "tags": ["n8n", "monitoring", "logcomex", "corrected"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Workflows Ativos",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_active_workflow_count{job=\"n8n-cluster\"}", "legendFormat": "Workflows"}
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Versão N8N",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {}
      },
      "targets": [
        {"expr": "n8n_version_info{job=\"n8n-cluster\"}", "legendFormat": "v{{version}}"}
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Função da Instância",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {}
      },
      "targets": [
        {"expr": "n8n_instance_role_leader{job=\"n8n-cluster\"}", "legendFormat": "Leader: {{value}}"}
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Tempo de Atividade",
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "dtdurations"}
      },
      "targets": [
        {"expr": "time() - n8n_process_start_time_seconds{job=\"n8n-cluster\"}", "legendFormat": "Ativo há {{value}}"}
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Uso de CPU - Cluster vs Workers",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "targets": [
        {"expr": "rate(n8n_process_cpu_seconds_total{job=\"n8n-cluster\"}[5m]) * 100", "legendFormat": "CPU Cluster"},
        {"expr": "rate(n8n_process_cpu_seconds_total{job=\"n8n-workers\"}[5m]) * 100", "legendFormat": "CPU Workers"}
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Uso de Memória - Cluster vs Workers",
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "decbytes"}
      },
      "targets": [
        {"expr": "n8n_process_resident_memory_bytes{job=\"n8n-cluster\"}", "legendFormat": "Memória Cluster"},
        {"expr": "n8n_process_resident_memory_bytes{job=\"n8n-workers\"}", "legendFormat": "Memória Workers"}
      ]
    },
    {
      "id": 7,
      "type": "gauge",
      "title": "Utilização de Memória - Cluster",
      "gridPos": {"x": 0, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0, "text": "Baixo"},
              {"color": "yellow", "value": 70, "text": "Médio"},
              {"color": "red", "value": 90, "text": "Alto"}
            ]
          }
        }
      },
      "targets": [
        {"expr": "(n8n_nodejs_heap_size_used_bytes{job=\"n8n-cluster\"} / n8n_nodejs_heap_size_total_bytes{job=\"n8n-cluster\"}) * 100", "legendFormat": "Heap % Cluster"}
      ]
    },
    {
      "id": 8,
      "type": "gauge",
      "title": "Utilização de Memória - Workers",
      "gridPos": {"x": 6, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0, "text": "Baixo"},
              {"color": "yellow", "value": 70, "text": "Médio"},
              {"color": "red", "value": 90, "text": "Alto"}
            ]
          }
        }
      },
      "targets": [
        {"expr": "(n8n_nodejs_heap_size_used_bytes{job=\"n8n-workers\"} / n8n_nodejs_heap_size_total_bytes{job=\"n8n-workers\"}) * 100", "legendFormat": "Heap % Workers"}
      ]
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Performance do Event Loop - Cluster vs Workers",
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 6},
      "fieldConfig": {
        "defaults": {"unit": "ms", "min": 0}
      },
      "targets": [
        {"expr": "n8n_nodejs_eventloop_lag_p50_seconds{job=\"n8n-cluster\"} * 1000", "legendFormat": "Latência Cluster (p50)"},
        {"expr": "n8n_nodejs_eventloop_lag_p50_seconds{job=\"n8n-workers\"} * 1000", "legendFormat": "Latência Workers (p50)"}
      ]
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "Limpeza de Memória (Garbage Collection)",
      "gridPos": {"x": 0, "y": 18, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "ms"}
      },
      "targets": [
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_sum{job=\"n8n-cluster\"}[5m]) / rate(n8n_nodejs_gc_duration_seconds_count{job=\"n8n-cluster\"}[5m]) * 1000", "legendFormat": "Tempo Médio Cluster"},
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_sum{job=\"n8n-workers\"}[5m]) / rate(n8n_nodejs_gc_duration_seconds_count{job=\"n8n-workers\"}[5m]) * 1000", "legendFormat": "Tempo Médio Workers"}
      ]
    },
    {
      "id": 11,
      "type": "table",
      "title": "Comparação Cluster vs Workers",
      "gridPos": {"x": 12, "y": 18, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_resources_total{job=\"n8n-cluster\"}", "legendFormat": "Recursos Cluster"},
        {"expr": "n8n_nodejs_active_resources_total{job=\"n8n-workers\"}", "legendFormat": "Recursos Workers"},
        {"expr": "n8n_process_open_fds{job=\"n8n-cluster\"}", "legendFormat": "Arquivos Cluster"},
        {"expr": "n8n_process_open_fds{job=\"n8n-workers\"}", "legendFormat": "Arquivos Workers"}
      ]
    }
  ]
}
