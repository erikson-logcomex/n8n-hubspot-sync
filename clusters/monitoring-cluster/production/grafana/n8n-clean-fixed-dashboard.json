{
  "id": 1,
  "uid": "n8n-monitoring-clean-fixed",
  "title": "Monitoramento N8N - Logcomex",
  "tags": ["n8n", "monitoring", "logcomex", "clean-fixed"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 2,
      "type": "stat",
      "title": "Workflows Ativos",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_active_workflow_count", "legendFormat": "Workflows"}
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Versão N8N",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_version_info", "legendFormat": "v{{version}}"}
      ]
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Tempo de Atividade",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "dtdurations"}
      },
      "targets": [
        {"expr": "time() - n8n_process_start_time_seconds", "legendFormat": "Ativo há {{value}}"}
      ]
    },
    {
      "id": 6,
      "type": "stat",
      "title": "Arquivos Abertos",
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_process_open_fds", "legendFormat": "Abertos: {{value}}"},
        {"expr": "n8n_process_max_fds", "legendFormat": "Máximo: {{value}}"}
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Uso de CPU",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "targets": [
        {"expr": "rate(n8n_process_cpu_seconds_total[5m]) * 100", "legendFormat": "CPU Total"},
        {"expr": "rate(n8n_process_cpu_user_seconds_total[5m]) * 100", "legendFormat": "CPU Usuário"},
        {"expr": "rate(n8n_process_cpu_system_seconds_total[5m]) * 100", "legendFormat": "CPU Sistema"}
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Uso de Memória",
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "decbytes"}
      },
      "targets": [
        {"expr": "n8n_process_resident_memory_bytes", "legendFormat": "Memória Física (RSS)"},
        {"expr": "n8n_nodejs_heap_size_used_bytes", "legendFormat": "Heap Usado"},
        {"expr": "n8n_nodejs_heap_size_total_bytes", "legendFormat": "Heap Total"}
      ]
    },
    {
      "id": 9,
      "type": "gauge",
      "title": "Utilização de Memória",
      "gridPos": {"x": 0, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0, "text": "Baixo"},
              {"color": "yellow", "value": 70, "text": "Médio"},
              {"color": "red", "value": 90, "text": "Alto"}
            ]
          }
        }
      },
      "targets": [
        {"expr": "(n8n_nodejs_heap_size_used_bytes / n8n_nodejs_heap_size_total_bytes) * 100", "legendFormat": "Heap %"}
      ]
    },
    {
      "id": 10,
      "type": "gauge",
      "title": "Utilização de CPU",
      "gridPos": {"x": 6, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0, "text": "Baixo"},
              {"color": "yellow", "value": 70, "text": "Médio"},
              {"color": "red", "value": 90, "text": "Alto"}
            ]
          }
        }
      },
      "targets": [
        {"expr": "rate(n8n_process_cpu_seconds_total[5m]) * 100", "legendFormat": "CPU %"}
      ]
    },
    {
      "id": 11,
      "type": "bargauge",
      "title": "Recursos Ativos",
      "gridPos": {"x": 12, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_resources_total", "legendFormat": "Total de Recursos"},
        {"expr": "n8n_nodejs_active_handles_total", "legendFormat": "Total de Handles"}
      ]
    },
    {
      "id": 12,
      "type": "bargauge",
      "title": "Arquivos Abertos vs Máximo",
      "gridPos": {"x": 18, "y": 12, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_process_open_fds", "legendFormat": "Arquivos Abertos"},
        {"expr": "n8n_process_max_fds", "legendFormat": "Máximo Permitido"}
      ]
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Performance do Event Loop",
      "gridPos": {"x": 0, "y": 18, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "ms", "min": 0}
      },
      "targets": [
        {"expr": "n8n_nodejs_eventloop_lag_p50_seconds * 1000", "legendFormat": "Latência Média (p50)"},
        {"expr": "n8n_nodejs_eventloop_lag_p90_seconds * 1000", "legendFormat": "Latência Alta (p90)"},
        {"expr": "n8n_nodejs_eventloop_lag_p99_seconds * 1000", "legendFormat": "Latência Crítica (p99)"}
      ]
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "Limpeza de Memória (Garbage Collection)",
      "gridPos": {"x": 12, "y": 18, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "ms"}
      },
      "targets": [
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_sum[5m]) / rate(n8n_nodejs_gc_duration_seconds_count[5m]) * 1000", "legendFormat": "Tempo Médio de Limpeza"},
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_count[5m])", "legendFormat": "Limpezas por Segundo"}
      ]
    },
    {
      "id": 15,
      "type": "piechart",
      "title": "Distribuição da Memória Heap",
      "gridPos": {"x": 0, "y": 26, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "decbytes"}
      },
      "targets": [
        {"expr": "n8n_nodejs_heap_space_size_used_bytes{space=\"old\"}", "legendFormat": "Memória Antiga"},
        {"expr": "n8n_nodejs_heap_space_size_used_bytes{space=\"new\"}", "legendFormat": "Memória Nova"},
        {"expr": "n8n_nodejs_heap_space_size_used_bytes{space=\"code\"}", "legendFormat": "Código"},
        {"expr": "n8n_nodejs_heap_space_size_used_bytes{space=\"large_object\"}", "legendFormat": "Objetos Grandes"},
        {"expr": "n8n_nodejs_heap_space_size_used_bytes{space=\"trusted\"}", "legendFormat": "Memória Confiável"}
      ]
    },
    {
      "id": 16,
      "type": "table",
      "title": "Resumo de Recursos",
      "gridPos": {"x": 12, "y": 26, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_resources_total", "legendFormat": "Total de Recursos"},
        {"expr": "n8n_nodejs_active_handles_total", "legendFormat": "Total de Handles"},
        {"expr": "n8n_process_open_fds", "legendFormat": "Arquivos Abertos"},
        {"expr": "n8n_process_max_fds", "legendFormat": "Máximo de Arquivos"}
      ]
    },
    {
      "id": 17,
      "type": "timeseries",
      "title": "Memória Externa",
      "gridPos": {"x": 0, "y": 34, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "decbytes"}
      },
      "targets": [
        {"expr": "n8n_nodejs_external_memory_bytes", "legendFormat": "Memória Externa"}
      ]
    },
    {
      "id": 18,
      "type": "timeseries",
      "title": "Recursos Ativos por Tipo",
      "gridPos": {"x": 12, "y": 34, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_resources{type=\"Timeout\"}", "legendFormat": "Timeouts"},
        {"expr": "n8n_nodejs_active_resources{type=\"TCPSocketWrap\"}", "legendFormat": "Conexões TCP"},
        {"expr": "n8n_nodejs_active_resources{type=\"TCPServerWrap\"}", "legendFormat": "Servidores TCP"},
        {"expr": "n8n_nodejs_active_resources{type=\"PipeWrap\"}", "legendFormat": "Pipes"},
        {"expr": "n8n_nodejs_active_resources{type=\"ProcessWrap\"}", "legendFormat": "Processos"}
      ]
    }
  ]
}


