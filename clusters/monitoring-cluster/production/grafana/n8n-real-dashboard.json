{
  "id": 1,
  "uid": "n8n-real-monitoring",
  "title": "N8N Real Monitoring",
  "tags": ["n8n", "monitoring", "real"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "N8N Status",
      "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "red", "value": 0},
              {"color": "green", "value": 1}
            ]
          }
        }
      },
      "targets": [
        {"expr": "up{job=\"n8n-cluster\"}", "legendFormat": "N8N Status"}
      ]
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Active Workflows",
      "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_active_workflow_count", "legendFormat": "Active"}
      ]
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Instance Role",
      "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_instance_role_leader", "legendFormat": "Leader"}
      ]
    },
    {
      "id": 4,
      "type": "stat",
      "title": "N8N Version",
      "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_version_info", "legendFormat": "v{{version}}"}
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "CPU Usage (Cores)",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "targets": [
        {"expr": "rate(n8n_process_cpu_seconds_total[5m])", "legendFormat": "Total CPU"},
        {"expr": "rate(n8n_process_cpu_user_seconds_total[5m])", "legendFormat": "User CPU"},
        {"expr": "rate(n8n_process_cpu_system_seconds_total[5m])", "legendFormat": "System CPU"}
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Memory Usage",
      "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "bytes"}
      },
      "targets": [
        {"expr": "n8n_process_resident_memory_bytes", "legendFormat": "RSS"},
        {"expr": "n8n_nodejs_heap_size_used_bytes", "legendFormat": "Heap Used"},
        {"expr": "n8n_nodejs_heap_size_total_bytes", "legendFormat": "Heap Total"}
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Event Loop Performance",
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "s", "min": 0}
      },
      "targets": [
        {"expr": "n8n_nodejs_eventloop_lag_p50_seconds", "legendFormat": "p50"},
        {"expr": "n8n_nodejs_eventloop_lag_p90_seconds", "legendFormat": "p90"},
        {"expr": "n8n_nodejs_eventloop_lag_p99_seconds", "legendFormat": "p99"},
        {"expr": "n8n_nodejs_eventloop_lag_mean_seconds", "legendFormat": "Mean"}
      ]
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Garbage Collection",
      "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "s"}
      },
      "targets": [
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_sum[5m]) / rate(n8n_nodejs_gc_duration_seconds_count[5m])", "legendFormat": "GC Duration"},
        {"expr": "rate(n8n_nodejs_gc_duration_seconds_count[5m])", "legendFormat": "GC Count/sec"}
      ]
    },
    {
      "id": 9,
      "type": "gauge",
      "title": "Memory Utilization %",
      "gridPos": {"x": 0, "y": 20, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "targets": [
        {"expr": "(n8n_nodejs_heap_size_used_bytes / n8n_nodejs_heap_size_total_bytes) * 100", "legendFormat": "Heap %"}
      ]
    },
    {
      "id": 10,
      "type": "gauge",
      "title": "CPU Utilization %",
      "gridPos": {"x": 6, "y": 20, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "targets": [
        {"expr": "rate(n8n_process_cpu_seconds_total[5m]) * 100", "legendFormat": "CPU %"}
      ]
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Open File Descriptors",
      "gridPos": {"x": 12, "y": 20, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_process_open_fds", "legendFormat": "Open FDs"},
        {"expr": "n8n_process_max_fds", "legendFormat": "Max FDs"}
      ]
    },
    {
      "id": 12,
      "type": "stat",
      "title": "Process Uptime",
      "gridPos": {"x": 18, "y": 20, "w": 6, "h": 6},
      "fieldConfig": {
        "defaults": {"unit": "s"}
      },
      "targets": [
        {"expr": "time() - n8n_process_start_time_seconds", "legendFormat": "Uptime"}
      ]
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Node.js Active Resources",
      "gridPos": {"x": 0, "y": 26, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_resources", "legendFormat": "{{type}}"},
        {"expr": "n8n_nodejs_active_resources_total", "legendFormat": "Total"}
      ]
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "Node.js Active Handles",
      "gridPos": {"x": 12, "y": 26, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "short"}
      },
      "targets": [
        {"expr": "n8n_nodejs_active_handles", "legendFormat": "{{type}}"},
        {"expr": "n8n_nodejs_active_handles_total", "legendFormat": "Total"}
      ]
    },
    {
      "id": 15,
      "type": "timeseries",
      "title": "Heap Space Usage",
      "gridPos": {"x": 0, "y": 34, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "bytes"}
      },
      "targets": [
        {"expr": "n8n_nodejs_heap_space_size_used_bytes", "legendFormat": "{{space}} Used"},
        {"expr": "n8n_nodejs_heap_space_size_total_bytes", "legendFormat": "{{space}} Total"}
      ]
    },
    {
      "id": 16,
      "type": "timeseries",
      "title": "External Memory",
      "gridPos": {"x": 12, "y": 34, "w": 12, "h": 8},
      "fieldConfig": {
        "defaults": {"unit": "bytes"}
      },
      "targets": [
        {"expr": "n8n_nodejs_external_memory_bytes", "legendFormat": "External Memory"}
      ]
    }
  ]
}


